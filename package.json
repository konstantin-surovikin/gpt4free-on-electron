{
  "name": "gpt4free-on-electron",
  "type": "module",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "build": {
    "asar": false,
    "productName": "GPT4Free on Electron",
    "artifactName": "gpt4free-on-electron.${ext}",
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "output": "build"
    },
    "linux": {
      "icon": "assets/img/icon.png"
    },
    "win": {
      "icon": "assets/img/icon.ico"
    },
    "mac": {
      "icon": "assets/img/icon.icns"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts' --fix"
  },
  "description": "",
  "devDependencies": {
    "gulp": "^5.0.0",
    "gulp-chmod": "^4.0.0",
    "gulp-copy": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "eslint": "^9.19.0",
    "@eslint/js": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "typescript-eslint": "^8.20.0"
  },
  "dependencies": {
    "globals": "^15.14.0",
    "node-netstat": "^1.9.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "wait-port": "^1.1.0"
  }
}
